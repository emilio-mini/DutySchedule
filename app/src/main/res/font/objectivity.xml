<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:app="http://schemas.android.com/apk/res-auto">
    <!-- Normal Styles -->
    <font
        app:fontStyle="normal"
        app:fontWeight="100"
        app:font="@font/objectivity_thin" />
    <font
        app:fontStyle="normal"
        app:fontWeight="300"
        app:font="@font/objectivity_light" />
    <font
        app:fontStyle="normal"
        app:fontWeight="400"
        app:font="@font/objectivity_regular" />
    <font
        app:fontStyle="normal"
        app:fontWeight="500"
        app:font="@font/objectivity_medium" />
    <font
        app:fontStyle="normal"
        app:fontWeight="700"
        app:font="@font/objectivity_bold" />
    <font
        app:fontStyle="normal"
        app:fontWeight="800"
        app:font="@font/objectivity_extrabold" />
    <font
        app:fontStyle="normal"
        app:fontWeight="900"
        app:font="@font/objectivity_black" />
    <font
        app:fontStyle="normal"
        app:fontWeight="950" 
        app:font="@font/objectivity_super" />

    <!-- Italic (Slanted) Styles -->
    <font
        app:fontStyle="italic"
        app:fontWeight="100"
        app:font="@font/objectivity_thin_slanted" />
    <font
        app:fontStyle="italic"
        app:fontWeight="300"
        app:font="@font/objectivity_light_slanted" />
    <font
        app:fontStyle="italic"
        app:fontWeight="400"
        app:font="@font/objectivity_regular_slanted" />
    <font
        app:fontStyle="italic"
        app:fontWeight="500"
        app:font="@font/objectivity_medium_slanted" />
    <font
        app:fontStyle="italic"
        app:fontWeight="700"
        app:font="@font/objectivity_bold_slanted" />
    <font
        app:fontStyle="italic"
        app:fontWeight="800"
        app:font="@font/objectivity_extrabold_slanted" />
    <font
        app:fontStyle="italic"
        app:fontWeight="900"
        app:font="@font/objectivity_black_slanted" />
    <font
        app:fontStyle="italic"
        app:fontWeight="950"
        app:font="@font/objectivity_super_slanted" />
</font-family>
